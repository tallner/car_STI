
{
    "AWSEBDockerrunVersion": 2,
    "containerDefinitions": [
      {
        "name": "app",
        "image": "774054463613.dkr.ecr.eu-north-1.amazonaws.com/app1",
        "hostname": "app",
        "essential": false,
        "memory": 128
      },
      {
        "name": "nginx",
        "image": "774054463613.dkr.ecr.eu-north-1.amazonaws.com/nginx1",
        "hostname": "nginx",
        "essential": true,
        "portMappings": [
          {
            "hostPort": 80,
            "containerPort": 80
          }
        ],
        "links": ["app"],
        "memory": 64
      }
    ]
  }